<form action='/search' method='POST'>
  <input type="text" id="search" placeholder="Search.." name="search">
  <button id="btn" type="submit"><i class="fa fa-search"></i></button>
</form>
<ul id="ul">
  <% products.forEach(product => { %>
    <li><%= product.name %> <%= product.price %> <a href="/products/<%= product.id %>">Details</a> <a href="/products/edit/<%= product.id %>">Edit</a> <button id="<%= product.id %>" class="delete">Delete</button>
  <%}) %>
</ul>